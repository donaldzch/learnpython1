<?xml version="1.0" encoding="UTF-8"?>
<appClient xmlns="http://www.ea.com/schema/eadp/nexus/data"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.ea.com/schema/eadp/nexus/data http://www.ea.com/schema/eadp/nexus/data/client.xsd">
    <universe>default</universe>
    <clientId>smoke_client</clientId>
    <clientSecret>secret1</clientSecret>
    <secretExpireBy>2014-01-02</secretExpireBy>
    <clientCertificateMappings>
        <clientCertificateMapping>
            <certificateSubject>123456</certificateSubject>
            <certificateIssuer>123456</certificateIssuer>
        </clientCertificateMapping>
        <clientCertificateMapping>
            <certificateSubject>123456</certificateSubject>
            <certificateIssuer>123456</certificateIssuer>
        </clientCertificateMapping>
    </clientCertificateMappings>
    <allowedRedirectUris>
        <uri>http://localhost/</uri>
        <uri>http://localhost:8080/</uri>
        <uri>http://www.ea.com/</uri>
        <uri>http://nucleus_callback/</uri>
    </allowedRedirectUris>
    <defaultRedirectUri>http://localhost/</defaultRedirectUri>
    <defaultRedirectUriForLogout>http://localhost:8080/</defaultRedirectUriForLogout>
    <allowedScopes>
        <scope>basic.identity</scope>
        <scope>basic.identity.write</scope>
        <scope>basic.persona</scope>
        <scope>basic.persona.write</scope>
        <scope>basic.entitlement</scope>
        <scope>basic.entitlement.write</scope>
        <scope>basic.encrypted_token</scope>
        <scope>basic.admin</scope>
        <scope>basic.utility</scope>
        <scope>basic.domaindata</scope>
        <scope>basic.commerce</scope>
        <scope>basic.commerce.write</scope>
        <scope>basic.billing</scope>
        <scope>basic.billing.write</scope>
        <scope>openid</scope>
        <scope>signin</scope>
        <scope>offline</scope>
        <scope>search.identity</scope>
        <scope>server.search.identity</scope>
        <scope>service.atom</scope>
        <scope>basic.catalog</scope>
        <scope>service.fifa-server</scope>
        <scope>basic.xbltoken</scope>
        <scope>basic.migration</scope>
        <scope>bulk.identity</scope>
    </allowedScopes>
    <defaultScopes>
        <scope>basic.identity</scope>
        <scope>basic.identity.write</scope>
        <scope>basic.persona</scope>
        <scope>basic.persona.write</scope>
        <scope>basic.entitlement</scope>
        <scope>basic.entitlement.write</scope>
        <scope>basic.encrypted_token</scope>
        <scope>basic.admin</scope>
        <scope>basic.utility</scope>
        <scope>basic.domaindata</scope>
        <scope>basic.commerce</scope>
        <scope>basic.commerce.write</scope>
        <scope>basic.billing</scope>
        <scope>basic.billing.write</scope>
        <scope>openid</scope>
        <scope>signin</scope>
        <scope>offline</scope>
        <scope>search.identity</scope>
        <scope>server.search.identity</scope>
        <scope>service.atom</scope>
        <scope>basic.catalog</scope>
        <scope>service.fifa-server</scope>
        <scope>basic.xbltoken</scope>
        <scope>basic.migration</scope>
        <scope>bulk.identity</scope>
    </defaultScopes>
    <reservedScopes>
        <scope>basic.billing</scope>
    </reservedScopes>
    <allowedResponseTypes>
        <responseType>token</responseType>
        <responseType>id_token</responseType>
        <responseType>code</responseType>
    </allowedResponseTypes>
    <allowedGrantTypes>
        <grantType>authorization_code</grantType>
        <grantType>exchange</grantType>
        <grantType>refresh_token</grantType>
        <grantType>client_credentials</grantType>
        <grantType>password</grantType>
    </allowedGrantTypes>
    <serviceScopeMapping>
        <serviceScope>
            <serviceName>service.identity</serviceName>
            <serviceScopeConfig>
                <scopes>
                    <scope>basic.identity</scope>
                    <scope>basic.identity.write</scope>
                </scopes>
            </serviceScopeConfig>
        </serviceScope>
    </serviceScopeMapping>

    <exchangeConfig>
        <trustedClients>
            <clientId>Fusion</clientId>
        </trustedClients>
    </exchangeConfig>

    <throttleConfigs>
        <throttleConfig>
            <configName>pid_put</configName>
            <configuration>
                <unitTime>10000</unitTime>
                <maximumRequest>10000000</maximumRequest>
                <accessState>allowed</accessState>
            </configuration>
        </throttleConfig>
        <throttleConfig>
            <configName>pid_get</configName>
            <configuration>
                <unitTime>10000</unitTime>
                <maximumRequest>10000000</maximumRequest>
                <accessState>allowed</accessState>
            </configuration>
        </throttleConfig>
    </throttleConfigs>
    <authTokenAccess>
        <enabled>true</enabled>
        <scopes>
            <scope>basic.identity</scope>
            <scope>basic.identity.write</scope>
        </scopes>
    </authTokenAccess>
    <directAccessConfig>
        <enabled>true</enabled>
        <userIdState>anyone</userIdState>
        <scopes>
            <scope>basic.identity</scope>
            <scope>basic.persona</scope>
            <scope>basic.entitlement</scope>
        </scopes>
        <allowIps>
            <ip>192.168.1.1</ip>
        </allowIps>
    </directAccessConfig>
    <logoutUris>
        <uri>http://localhost/clear/sid</uri>
    </logoutUris>
    <clientCredentialsConfig>
        <pidIdState>anyone</pidIdState>
    </clientCredentialsConfig>

    <allowedClientIds>
        <clientId>Fusion</clientId>
    </allowedClientIds>

    <allowedRegistrationSources>
        <registrationSource>nexus</registrationSource>
    </allowedRegistrationSources>

    <defaultRegistrationSource>nexus</defaultRegistrationSource>
    <allowedAuthenticationSources>
        <authenticationSource>1234</authenticationSource>
    </allowedAuthenticationSources>
    <defaultAuthenticationSource>1234</defaultAuthenticationSource>
    <allowedDisplays>
        <displayType>console/welcome</displayType>
    </allowedDisplays>
    <defaultDisplay>console/welcome</defaultDisplay>
    <defaultLocale>1234</defaultLocale>
    <defaultTheme>pc</defaultTheme>
    <authorizationCodeExpiration>10000tt</authorizationCodeExpiration>
    <accessTokenExpiration>10000tt</accessTokenExpiration>
    <refreshTokenExpiration>10000</refreshTokenExpiration>
    <idTokenExpiration>10000</idTokenExpiration>
    <authorizationCodeConfig>
        <redirectUriCheck>true</redirectUriCheck>
    </authorizationCodeConfig>
    <passwordConfig>
        <userIPHeaderEnabled>true</userIPHeaderEnabled>
    </passwordConfig>
    <recaptchaDisabled>true</recaptchaDisabled>
    <ipWhitelists>
        <ip>192.168.1.1</ip>
    </ipWhitelists>
    <apiIpWhitelists>
        <apiIpWhitelist>
            <apiName>pid_get</apiName>
            <apiIpWhitelistConfig>
                <ips>
                    <ip>10.88.1.1</ip>
                </ips>
            </apiIpWhitelistConfig>
        </apiIpWhitelist>
    </apiIpWhitelists>

    <loginRequiredForAccessToken>true</loginRequiredForAccessToken>

    <pointOfContacts>hello@ea.com</pointOfContacts>

    <psnTicketExpirationCheckDisabled>true</psnTicketExpirationCheckDisabled>
    <pidStatusCheckConfig>
        <enabled>true</enabled>
        <loginRequiredOnFailure>true</loginRequiredOnFailure>
        <allowedUserStatus>
            <status>ACTIVE</status>
        </allowedUserStatus>
        <allowedPersonaStatus>
            <status>ACTIVE</status>
        </allowedPersonaStatus>
    </pidStatusCheckConfig>
    <legalDocCheckConfig>
        <enabled>true</enabled>
        <gameIdentifier>default</gameIdentifier>
    </legalDocCheckConfig>
    <underageCheckConfig>
        <enabled>true</enabled>
        <ageUpEnabled>true</ageUpEnabled>
        <loginRequiredOnFailure>true</loginRequiredOnFailure>
        <overrideEntitlements>
            <entitlement>
                <entitlementTag>123</entitlementTag>
                <entitlementType>SUBSCRIPTIONS</entitlementType>
                <group>123</group>
                <productId>1234</productId>
                <projectId>1234</projectId>
            </entitlement>
        </overrideEntitlements>
    </underageCheckConfig>
    <lastAuthDateUpdateCoolDownInSeconds>300</lastAuthDateUpdateCoolDownInSeconds>
    <deviceProfileCheckConfig>
        <enabled>true</enabled>
    </deviceProfileCheckConfig>
    <entitlementCheckConfig>
        <enabled>true</enabled>
        <loginRequiredOnFailure>true</loginRequiredOnFailure>
        <enforced>true</enforced>
        <entitlements>
            <entitlement>
                <checkType>periodTrial</checkType>
                <projectId>1234</projectId>
                <productId>1234</productId>
                <autoGrant>true</autoGrant>
                <entitlementTag>1234</entitlementTag>
                <entitlementType>ONLINE_ACCESS</entitlementType>
                <group>hello</group>
                <useManagedLifeCycle>true</useManagedLifeCycle>
                <trialTimeInSeconds>300</trialTimeInSeconds>
                <trialStart>2014-01-02T00:00:00</trialStart>
                <trialEnd>2014-02-01T00:00:00</trialEnd>
            </entitlement>
        </entitlements>
    </entitlementCheckConfig>
    <preferenceOptins>
        <optin>1234</optin>
        <optin>12345</optin>
    </preferenceOptins>
    <allowedMobileLoginTypes>
        <mobileLoginType>mobile_game_UPID</mobileLoginType>
        <mobileLoginType>mobile_game_facebook</mobileLoginType>
        <mobileLoginType>mobile_game_origin</mobileLoginType>
        <mobileLoginType>origin</mobileLoginType>
    </allowedMobileLoginTypes>
    <twoFactorAuthenticationEnabled>true</twoFactorAuthenticationEnabled>
    <upgradeNeededCheckConfig>
        <enabled>true1</enabled>
    </upgradeNeededCheckConfig>
    <linkingStatusCheckConfig>
        <enabled>true1</enabled>
    </linkingStatusCheckConfig>
    <externalStyleSheets>
        <source>https:////afdasf</source>
    </externalStyleSheets>
</appClient>